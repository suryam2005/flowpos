{
  "expo": {
    "name": "FlowPOS",
    "slug": "flowpos",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon-new.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flowpos.app",
      "requireFullScreen": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-new.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flowpos.app",
      "permissions": [
        "CAMERA",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "VIBRATE",
        "READ_SMS",
        "RECEIVE_SMS",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.READ_SMS",
        "android.permission.RECEIVE_SMS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon-new.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "eb41bbcd-45df-40f7-9943-061a0b32158a"
      }
    },
    "owner": "suryamuralirajan"
  }
}
